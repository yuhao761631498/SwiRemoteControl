    <!--登录注册输入框-->
    <style name="TextInputLayout_Text_Style">
        <item name="android:textSize">@dimen/pf_text_size_20</item>
        <item name="android:textColor">@color/pf_color_a8a8a9</item>
    </style>

    <!--媒体库 编辑 分割线 样式-->
    <style name="Media_Edit_Division_line_Style">
        <item name="android:layout_height">@dimen/pf_ui_size_1</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/pf_color_282828</item>
    </style>
